#: applications/luci-app-user/root/usr/share/luci/menu.d/luci-app-user.json:3
msgid "User Password"
msgstr "felhasználói jelszó "

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:78
msgid "user group"
msgstr "user csoport"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:267
msgid "Cannot encrypt plaintext password since uhttpd is not installed."
msgstr ""
"Nem lehet titkosítani az egyszerű szöveges jelszót, mivel az uhttpd nincs "
"telepítve."

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:79
msgid "Description"
msgstr "Leírás"

#: applications/luci-app-user/root/usr/share/rpcd/user.d/luci-app-user.json:3
msgid "Grant access to user configuration"
msgstr "Hozzáférés biztosítása az user beállításhoz"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:212
msgid "Login name"
msgstr "Bejelentkezési név"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:202
#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:209
msgid "LuCI Logins"
msgstr "LuCI bejelentkezések"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:244
msgid "Password value"
msgstr "Jelszóérték"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:215
msgid "Password variant"
msgstr "Jelszóváltozat"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:259
msgid "The password may not start with \"$p$\"."
msgstr "A jelszó nem kezdődhet „$p$” értékkel."

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:232
msgid "The system account to use the password from"
msgstr "Az a rendszerfiók, amelytől a jelszót használja"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:232
msgid "UNIX account"
msgstr "UNIX fiók"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:282
msgid "Unable to encrypt plaintext password: %s"
msgstr "Nem lehet titkosítani az egyszerű szöveges jelszót: %s"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:217
msgid "Use UNIX password in /etc/shadow"
msgstr "Az /etc/shadow fájlban lévő UNIX jelszó használata"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:218
msgid "Use encrypted password hash"
msgstr "Titkosított jelszókivonat használata"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:219
msgid "Use plain password"
msgstr "Egyszerű jelszó használata"
