#: applications/luci-app-user/root/usr/share/luci/menu.d/luci-app-user.json:3
msgid "User Password"
msgstr "Потребителска парола"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:267
msgid "Cannot encrypt plaintext password since uhttpd is not installed."
msgstr ""
"Текстът на паролата не може да бъде кодиран, защото uhttpd не е инсталиран."

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:79
msgid "Description"
msgstr "Описание"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:212
msgid "Login name"
msgstr "Потребителско име"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:202
#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:209
msgid "LuCI Logins"
msgstr "LuCI Потребителски имена"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:209
msgid "New account"
msgstr "Нов акаунт"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:244
msgid "Password value"
msgstr "Стойност на паролата"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:215
msgid "Password variant"
msgstr "Вариант на паролата"

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:259
msgid "The password may not start with \"$p$\"."
msgstr "Паролата не може да започва с \"$p$\"."

#: applications/luci-app-user/htdocs/luci-static/resources/view/system/user.js:232
